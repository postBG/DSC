from HW6.P4 import Calculator
c = Calculator()
c.digit(0)
c.digit(0)
c.digit(1)
c.digit(2)
c.plus()
c.minus()
c.digit(3)
c.digit(1)
c.digit(2)
c.plus()
c.minus()
c.minus()
c.plus()
c.digit(0)
c.digit(0)
c.digit(1)
c.digit(0)
c.digit(0)
assert c.equal() == -200


c = Calculator()
c.clear()
c.digit(9)
c.plus()
c.digit(1)
c.digit(2)
c.digit(5)
c.minus()
c.minus()
c.digit(3)
c.plus()
c.clear()
c.digit(5)
c.digit(2)
c.minus()
c.digit(5)
c.minus()
c.plus()
c.digit(2)
assert c.equal() == 49


c = Calculator()
c.clear()
assert c.equal() == 0

c = Calculator()
assert c.equal() == 0

c = Calculator()
c.clear()
c.clear()
c.digit(9)
c.plus()
c.digit(1)
c.digit(2)
c.digit(5)
c.minus()
c.minus()
c.digit(3)
c.plus()
c.clear()
c.digit(5)
c.digit(2)
c.minus()
c.digit(5)
c.minus()
c.plus()
c.digit(2)
assert c.equal() == 49
assert c.equal() == 49
c.clear()
assert c.equal() == 0


c = Calculator()
c.digit(0)
c.digit(0)
c.digit(5)
c.digit(0)
c.digit(2)
assert c.equal() == 502